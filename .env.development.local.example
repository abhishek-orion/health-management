# API Simulation Configuration for Development
# Copy this file to .env.development.local to customize

# Enable/disable API simulation (true/false)
REACT_APP_API_SIMULATION=true

# Latency range in milliseconds
REACT_APP_API_MIN_LATENCY=200
REACT_APP_API_MAX_LATENCY=2000

# Error rate as percentage (0-100)
# 5 = 5% chance of random errors
REACT_APP_API_ERROR_RATE=5